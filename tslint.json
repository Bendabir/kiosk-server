{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "trailing-comma": [ false ],
        "no-console": [ false ],
        "interface-name": [ false ],
        "max-classes-per-file": [ false, 1 ],
        "no-implicit-dependencies": [true, "dev"]
    },
    "rulesDirectory": [],
    "max-line-length": [
        true,
        {
          "limit": 120,
          "ignore-pattern": "^import |^export {(.*?)}",
          "check-strings": true,
          "check-regex": true
        }
    ]
}
